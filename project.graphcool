type File implements Node {
  contentType: String!
  createdAt: DateTime!
  id: ID! @isUnique
  name: String!
  secret: String! @isUnique
  size: Int!
  updatedAt: DateTime!
  url: String! @isUnique
}

type User implements Node {
  createdAt: DateTime!
  id: ID! @isUnique
  updatedAt: DateTime!
  email: String @isUnique
  password: String
}

type Room implements Node {
  name: String!
  identifier: String! @isUnique
  bookings: [Booking!]! @relation(name: "BookingOnRoom")
  createdAt: DateTime!
  id: ID! @isUnique
  updatedAt: DateTime!
  availableForBooking: Boolean
}

type Booking implements Node {
  start: DateTime!
  end: DateTime!
  room: Room! @relation(name: "BookingOnRoom")
  createdAt: DateTime!
  id: ID! @isUnique
  updatedAt: DateTime!
}